{
	"plugins": ["testing-library", "jest-dom"],
	"extends": [
		"react-app",
		"react-app/jest",
		"plugin:testing-library/react",
		"plugin:jest-dom/recommended"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	}
}
